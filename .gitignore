# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp
/jetty
/imports
.DS_Store

# Ignore config yml files
config/*.yml

# Ignore containerizing ENV file
.env
.env_phalt

# Ignore custom git instructions
/app/views/repos/_git_instructions_custom.html.erb

# Ignore featured objects directory
/public/featured

# Ignore bash script
repo.sh
just_jetty.sh

# Ignore rdf
*.rdf

#Ignore docker
/docker

# Other stuff
*.rdb
/.idea
*.csv
